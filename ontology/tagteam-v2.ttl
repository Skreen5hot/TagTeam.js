@prefix : <http://tagteam.fandaws.org/ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix bfo: <http://purl.obolibrary.org/obo/> .
@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

# =============================================================================
# TagTeam v2 Ontology â€” Structural Semantics & Normalization
# =============================================================================
#
# Extends BFO/CCO with vocabulary for:
# - Speech act ICE nodes (Inquiry, Conditional, Exclamatory)
# - Clause segmentation and linking
# - Verb class routing (psych-verbs, raising, causatives)
# - Discourse memory and anaphora resolution
#
# Namespace: http://tagteam.fandaws.org/ontology/
# Alignment: BFO 2020, CCO
# Imports: tagteam-core.ttl
# =============================================================================

<http://tagteam.fandaws.org/ontology/v2/>
    a owl:Ontology ;
    owl:versionIRI <http://tagteam.fandaws.org/ontology/v2/2.0.0/> ;
    owl:versionInfo "2.0.0"^^xsd:string ;
    dc:title "TagTeam v2 Ontology"@en ;
    dc:description "Structural semantics extension for clause segmentation, speech act typing, verb class routing, and discourse memory."@en ;
    dcterms:created "2026-02-01"^^xsd:date ;
    owl:imports <http://tagteam.fandaws.org/ontology/core/> .


# =============================================================================
# SPEECH ACT CLASSES
# =============================================================================

:SpeechAct
    a owl:Class ;
    rdfs:subClassOf cco:InformationContentEntity ;
    rdfs:label "Speech Act"@en ;
    skos:prefLabel "Speech Act"@en ;
    skos:definition "An information content entity that denotes the illocutionary force of an utterance."@en .

:Inquiry
    a owl:Class ;
    rdfs:subClassOf :SpeechAct ;
    rdfs:label "Inquiry"@en ;
    skos:prefLabel "Inquiry"@en ;
    skos:definition "A speech act that requests information by wrapping an interrogative act."@en .

:ConditionalContent
    a owl:Class ;
    rdfs:subClassOf :SpeechAct ;
    rdfs:label "Conditional Content"@en ;
    skos:prefLabel "Conditional Content"@en ;
    skos:definition "A speech act that denotes a condition-consequence relationship where the truth or occurrence of one proposition depends upon the truth or occurrence of another proposition."@en .

:ValueAssertionEvent
    a owl:Class ;
    rdfs:subClassOf :SpeechAct ;
    rdfs:label "Value Assertion Event"@en ;
    skos:prefLabel "Value Assertion Event"@en ;
    skos:definition "A speech act that expresses a judgment about the worth, quality, or significance of some entity."@en .


# =============================================================================
# STRUCTURAL CLASSES
# =============================================================================

:ClauseRelation
    a owl:Class ;
    rdfs:label "Clause Relation"@en ;
    skos:prefLabel "Clause Relation"@en ;
    skos:definition "A structural connection that links two grammatical units within a compound or complex sentence."@en .

:CausativeAct
    a owl:Class ;
    rdfs:subClassOf cco:IntentionalAct ;
    rdfs:label "Causative Act"@en ;
    skos:prefLabel "Causative Act"@en ;
    skos:definition "An intentional behavior that brings about another agent's performance of an action."@en .


# =============================================================================
# ACTUALITY STATUS INDIVIDUALS (new to v2; others reused from core)
# =============================================================================

:Interrogative
    a owl:NamedIndividual, :ActualityStatus ;
    rdfs:label "Interrogative"@en ;
    skos:prefLabel "Interrogative"@en ;
    skos:definition "An actuality status that denotes the event is being questioned."@en .


# =============================================================================
# CLAUSE RELATION TYPE INDIVIDUALS
# =============================================================================

:and_then
    a owl:NamedIndividual, :ClauseRelation ;
    rdfs:label "and then"@en ;
    skos:prefLabel "and then"@en ;
    skos:definition "A clause relation denoting sequential coordination where clause A is followed by clause B."@en .

:therefore
    a owl:NamedIndividual, :ClauseRelation ;
    rdfs:label "therefore"@en ;
    skos:prefLabel "therefore"@en ;
    skos:definition "A clause relation denoting that clause B is a consequence of clause A."@en .

:in_order_that
    a owl:NamedIndividual, :ClauseRelation ;
    rdfs:label "in order that"@en ;
    skos:prefLabel "in order that"@en ;
    skos:definition "A clause relation denoting a purposive connection where one clause expresses the goal or intended outcome of another clause."@en .

:contrasts_with
    a owl:NamedIndividual, :ClauseRelation ;
    rdfs:label "contrasts with"@en ;
    skos:prefLabel "contrasts with"@en ;
    skos:definition "A clause relation denoting that clause B presents information that differs from or opposes the information presented in clause A."@en .

:alternative_to
    a owl:NamedIndividual, :ClauseRelation ;
    rdfs:label "alternative to"@en ;
    skos:prefLabel "alternative to"@en ;
    skos:definition "A clause relation denoting that clause B is a substitute option for clause A."@en .


# =============================================================================
# TEMPORAL RELATION INDIVIDUALS
# =============================================================================

:precedes
    a owl:NamedIndividual ;
    rdfs:label "precedes"@en ;
    skos:prefLabel "precedes"@en ;
    skos:definition "A temporal relation denoting that the source event occurs before the target event."@en .

:follows
    a owl:NamedIndividual ;
    rdfs:label "follows"@en ;
    skos:prefLabel "follows"@en ;
    skos:definition "A temporal relation denoting that the source event occurs after the target event."@en .

:simultaneous_with
    a owl:NamedIndividual ;
    rdfs:label "simultaneous with"@en ;
    skos:prefLabel "simultaneous with"@en ;
    skos:definition "A temporal relation denoting that the events occur at the same time."@en .


# =============================================================================
# DATATYPE PROPERTIES
# =============================================================================

:clauseIndex
    a owl:DatatypeProperty ;
    rdfs:label "clause index"@en ;
    skos:prefLabel "clause index"@en ;
    skos:definition "A datatype property that denotes a zero-based numerical position identifying which grammatical unit an act belongs to in a multi-unit sentence."@en ;
    rdfs:range xsd:integer .

:subjectSource
    a owl:DatatypeProperty ;
    rdfs:label "subject source"@en ;
    skos:prefLabel "subject source"@en ;
    skos:definition "A datatype property that denotes the method by which a grammatical topic was determined for a linguistic expression."@en ;
    rdfs:range xsd:string .

:whPhrase
    a owl:DatatypeProperty ;
    rdfs:label "Wh-phrase"@en ;
    skos:prefLabel "Wh-phrase"@en ;
    skos:definition "A datatype property that denotes the original interrogative phrase from a question before normalization."@en ;
    rdfs:range xsd:string .

:verbClass
    a owl:DatatypeProperty ;
    rdfs:label "verb class"@en ;
    skos:prefLabel "verb class"@en ;
    skos:definition "A datatype property that denotes the grammatical category classification of action words, such as psychological, raising, or causative types."@en ;
    rdfs:range xsd:string .

:epistemicStatus
    a owl:DatatypeProperty ;
    rdfs:label "epistemic status"@en ;
    skos:prefLabel "epistemic status"@en ;
    skos:definition "A datatype property that denotes a qualification concerning the degree of certainty or confidence associated with a proposition or belief state."@en ;
    rdfs:range xsd:string .

:isQuestionFocus
    a owl:DatatypeProperty ;
    rdfs:label "is question focus"@en ;
    skos:prefLabel "is question focus"@en ;
    skos:definition "A datatype property that indicates the entity is the primary subject of inquiry in an interrogative statement."@en ;
    rdfs:range xsd:boolean .


# =============================================================================
# ANNOTATION PROPERTIES
# =============================================================================

:structuralAmbiguity
    a owl:AnnotationProperty ;
    rdfs:label "structural ambiguity"@en ;
    skos:prefLabel "structural ambiguity"@en ;
    skos:definition "An annotation that flags a node as having unresolved structural ambiguity from the v2 pre-processing layer."@en .


# =============================================================================
# OBJECT PROPERTIES
# =============================================================================

:relationType
    a owl:ObjectProperty ;
    rdfs:label "relation type"@en ;
    skos:prefLabel "relation type"@en ;
    skos:definition "An object property that links a clause connection to its semantic category, such as sequential, causal, or contrastive."@en ;
    rdfs:domain :ClauseRelation .

:fromClause
    a owl:ObjectProperty ;
    rdfs:label "from clause"@en ;
    skos:prefLabel "from clause"@en ;
    skos:definition "An object property that relates a relational entity to its source linguistic act."@en ;
    rdfs:domain :ClauseRelation .

:toClause
    a owl:ObjectProperty ;
    rdfs:label "to clause"@en ;
    skos:prefLabel "to clause"@en ;
    skos:definition "An object property that relates a conditional statement to its consequent linguistic act."@en ;
    rdfs:domain :ClauseRelation .

:has_antecedent
    a owl:ObjectProperty ;
    rdfs:label "has antecedent"@en ;
    skos:prefLabel "has antecedent"@en ;
    skos:definition "An object property that relates a conditional information content entity to the information content entity that denotes the condition upon which the conditional depends."@en ;
    rdfs:domain :ConditionalContent .

:has_consequent
    a owl:ObjectProperty ;
    rdfs:label "has consequent"@en ;
    skos:prefLabel "has consequent"@en ;
    skos:definition "An object property that relates a conditional information content entity to the information content entity that denotes what follows when the conditional's antecedent condition is satisfied."@en ;
    rdfs:domain :ConditionalContent .

:has_cause
    a owl:ObjectProperty ;
    rdfs:label "has cause"@en ;
    skos:prefLabel "has cause"@en ;
    skos:definition "An object property that relates a mental process to the entity that causally brings about that mental process."@en .


# =============================================================================
# END OF V2 ONTOLOGY
# =============================================================================
