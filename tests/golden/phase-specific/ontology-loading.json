      "category": "ttl-basic-class",
      "input": {
        "format": "ttl",
        "content": "@prefix tagteam: <http://tagteam.fandaws.org/ontology/> .\n@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .\n\ntagteam:MedicalEvent a rdfs:Class ;\n  rdfs:label \"Medical Event\" ;\n  rdfs:comment \"An event in a medical context\" ."
      },
      "expectedOutput": {
        "loaded": true,
        "classCount": 1,
        "classes": [
          {
            "uri": "http://tagteam.fandaws.org/ontology/MedicalEvent",
            "label": "Medical Event",
            "comment": "An event in a medical context",
            "type": "Class"
          }
        ],
        "errors": []
      },
      "tags": ["ontology", "ttl", "basic", "class-definition", "phase6"],
      "priority": "P0",
      "notes": "Basic TTL class definition"
    },
