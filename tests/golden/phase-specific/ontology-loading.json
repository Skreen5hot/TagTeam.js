    {
      "id": "ontology-merge-002",
      "category": "merge-conflict",
      "input": {
        "operation": "merge",
        "sources": [
          {
            "name": "source1.json",
            "namespace": "http://example.org/",
            "classes": [{"id": "Event", "label": "Event Type 1"}]
          },
          {
            "name": "source2.json",
            "namespace": "http://example.org/",
            "classes": [{"id": "Event", "label": "Event Type 2"}]
          }
        ],
        "strategy": "prefer-first"
      },
      "expectedOutput": {
        "merged": true,
        "conflicts": [
          {
            "type": "duplicate-class",
            "id": "Event",
            "resolution": "prefer-first",
            "kept": "Event Type 1",
            "discarded": "Event Type 2"
          }
        ],
        "errors": []
      },
      "tags": ["ontology", "merge", "conflict", "resolution", "phase6"],
      "priority": "P0",
      "notes": "Merge with namespace conflict - prefer first strategy"
    },
