      "expectedOutput": {
        "entity": "surgeon",
        "definiteness": "definite",
        "determiner": "the",
        "modifiers": ["only"],
        "scarcityMarker": "only",
        "entityType": "cco:Person"
      },
      "tags": ["definite", "scarcity", "medical"],
      "priority": "P0",
      "notes": "Scarcity marker 'only' implies uniqueness and scarcity"
    },
    {
