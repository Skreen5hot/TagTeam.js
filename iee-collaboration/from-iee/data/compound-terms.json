  "normalizationRules": {
    "description": "How to normalize compound terms for entity matching",
    "rules": [
      {
        "rule": "Replace spaces with underscores",
        "example": "life support → life_support"
      },
      {
        "rule": "Convert to lowercase",
        "example": "Life Support → life_support"
      },
      {
        "rule": "Remove hyphens in compounds",
        "example": "end-of-life → end_of_life"
      },
      {
        "rule": "Preserve apostrophes in possessives",
        "example": "patient's rights → patients_rights (normalize to plural form)"
      }
    ]
  },
  "parserGuidance": {
    "detection": "Use NER (Named Entity Recognition) and compound noun detection before tokenization",
    "chunking": "Treat these as single noun phrases, do not split during tokenization",
    "posTagging": "Tag entire compound as NN (noun) or NNP (proper noun)",
    "examples": [
      {
        "input": "The patient is on life support",
        "correctTokenization": ["The", "patient", "is", "on", "life support"],
        "incorrectTokenization": ["The", "patient", "is", "on", "life", "support"]
      },
      {
        "input": "Climate change affects future generations",
        "correctTokenization": ["Climate change", "affects", "future generations"],
        "incorrectTokenization": ["Climate", "change", "affects", "future", "generations"]
      }
    ]
  },
  "testCases": [
    {
      "description": "Multi-word entities must be preserved as single semantic units",
      "input": "The family must decide about life support",
      "expected": {
        "agent": "family",
        "action": "decide",
        "patient": "life_support"
      },
      "incorrect": {
        "patient": "support"
      }
    },
    {
      "description": "Nested compounds should be recognized",
      "input": "Informed consent for experimental treatment",
      "expected": {
        "compounds": ["informed_consent", "experimental_treatment"]
      }
    },
    {
      "description": "Context-dependent compounds",
      "input": "The coal plant provides job security",
      "expected": {
        "compounds": ["coal_plant", "job_security"]
      }
    }
  ]
