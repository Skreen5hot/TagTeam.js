{
  "name": "tagteam-js",
  "version": "3.0.0-alpha.1",
  "description": "Deterministic semantic parser with ethical value detection - BFO-compatible ontology system",
  "main": "dist/tagteam.js",
  "bin": {
    "tagteam-compile": "./bin/tagteam-ontology-compiler.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "test": "node tests/integration/test-full-corpus.js",
    "test:browser": "echo 'Open tests/browser/verify-bundle.html in your browser'",
    "test:integration": "node tests/integration/test-integration.js",
    "test:ttl": "node tests/unit/ttl-parser.test.js",
    "test:ttl-integration": "node tests/integration/ttl-parser-integration.test.js",
    "test:cli-compiler": "node tests/integration/cli-compiler.test.js",
    "test:real-ontologies": "node tests/integration/real-ontologies.test.js",
    "metrics": "node scripts/calculate-metrics.js",
    "compile-ontology": "node bin/tagteam-ontology-compiler.js",
    "verify:ontology": "node -e \"const {verifyOntologyIntegrity}=require('./src/security/ontology-integrity'); const r=verifyOntologyIntegrity('ontology-manifest.json'); console.log(JSON.stringify(r,null,2)); if(!r.valid)process.exit(1)\"",
    "security:test": "node tests/unit/security/input-validator.test.js && node tests/unit/security/ontology-integrity.test.js && node tests/unit/security/semantic-validators.test.js && node tests/unit/security/output-sanitizer.test.js && node tests/unit/security/audit-logger.test.js && node tests/unit/security/verb-taxonomy.test.js && node tests/unit/security/red-team.test.js",
    "test:v2": "node tests/unit/v2/run-all.js",
    "test:v2:integration": "node tests/integration/v2/run-all.js",
    "test:golden": "node tests/golden/run-golden-tests.js",
    "test:golden:report": "node tests/golden/run-golden-tests.js && node tests/golden/generate-enhanced-report.js",
    "test:golden:full": "node tests/golden/run-golden-tests.js && node tests/golden/generate-enhanced-report.js && node tests/golden/detect-regressions.js",
    "test:expert": "node tests/expert-validation/run-expert-tests.js",
    "test:expert:open": "echo 'Opening expert test harness...' && start tests/expert-validation/expert-test-harness.html",
    "test:component": "node tests/component/run-component-tests.js",
    "test:component:verbose": "node tests/component/run-component-tests.js --verbose",
    "report:golden": "node tests/golden/generate-enhanced-report.js",
    "report:component": "node tests/component/run-component-tests.js",
    "regression:check": "node tests/golden/detect-regressions.js",
    "validate:golden": "node tests/golden/run-golden-tests.js --dry-run",
    "validate:schema": "node tests/golden/run-golden-tests.js --dry-run"
  },
  "keywords": [
    "nlp",
    "ethics",
    "semantic-parsing",
    "values",
    "bfo",
    "ontology",
    "philosophy",
    "moral-reasoning",
    "deliberation"
  ],
  "author": "Aaron Damiano",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/TagTeam.js"
  },
  "bugs": {
    "url": "https://github.com/yourusername/TagTeam.js/issues"
  },
  "homepage": "https://github.com/yourusername/TagTeam.js#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "compromise": "^14.14.5",
    "n3": "^2.0.1"
  }
}
